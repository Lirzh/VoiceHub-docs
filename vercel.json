{
  "name": "voicehub-docs",
  "version": 2,
  "buildCommand": "pnpm install --frozen-lockfile && pnpm run docs:build",
  "outputDirectory": "src/.vuepress/dist",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/(.*\\.html|.*\\.js|.*\\.css|.*\\.png|.*\\.jpg|.*\\.jpeg|.*\\.gif|.*\\.ico|.*\\.svg|.*\\.woff|.*\\.woff2|.*\\.ttf|.*\\.eot|.*\\.json)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/[^.]+",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
